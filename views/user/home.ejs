<section id="calculator" class="calculator-section py-5">
  <div class="container">
    <h2 class="text-center mb-4">Calculate Your Solar Savings</h2>
    <div class="row justify-content-center">
      <div class="col-md-8">

        <form id="solarCalcForm" class="border p-4 rounded bg-white shadow-sm">
          <div class="mb-3">
            <label for="monthlyBill" class="form-label">Monthly Electricity Bill (₹)</label>
            <input type="number" class="form-control" id="monthlyBill" placeholder="e.g., 5000" required>
          </div>
          <div class="mb-3">
            <label for="roofArea" class="form-label">Rooftop Area (sq ft)</label>
            <input type="number" class="form-control" id="roofArea" placeholder="e.g., 400" required>
          </div>
          <div class="mb-3">
            <label for="systemSize" class="form-label">Estimated System Size (kW)</label>
            <input type="number" class="form-control" id="systemSize" placeholder="Enter size ">
          </div>
          <button type="button" class="btn btn-primary w-100" onclick="calculateSavings()">Calculate</button>
        </form>

        <div id="calcResults" class="mt-4 p-4 bg-light rounded d-none">
          <h4>Estimated Savings & Info</h4>
          <ul class="list-unstyled">
            <li><strong>Monthly Energy from Solar:</strong> <span id="energyOutput">–</span> kWh</li>
            <li><strong>Monthly Cost Savings:</strong> ₹<span id="costSaving">–</span></li>
            <li><strong>Estimated Payback Period:</strong> <span id="paybackPeriod">–</span> years</li>
          </ul>
        </div>

      </div>
    </div>
  </div>
</section>
<script>
  function calculateSavings() {
    const bill = parseFloat(document.getElementById('monthlyBill').value);
    const area = parseFloat(document.getElementById('roofArea').value);
    let size = parseFloat(document.getElementById('systemSize').value);

    // Auto-calc system size if not provided (1 kW per ~100 sq ft)
    if (!size && area > 0) size = (area / 100).toFixed(1);

    // Estimate: 120 kWh per kW installed monthly
    const energy = (size * 120).toFixed(0);
    const cost = (energy * (bill / 30) / (parseFloat(document.getElementById('monthlyBill').value) / parseFloat(document.getElementById('monthlyBill').value))).toFixed(0);
    const payback = ((size * 60000) / (cost * 12)).toFixed(1); // ₹60,000 per kW placeholder

    document.getElementById('systemSize').value = size;
    document.getElementById('energyOutput').textContent = energy;
    document.getElementById('costSaving').textContent = cost;
    document.getElementById('paybackPeriod').textContent = payback;

    document.getElementById('calcResults').classList.remove('d-none');
  }
</script>


